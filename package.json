{
  "name": "node-bin-manager",
  "version": "0.1.0",
  "description": "Manage remote binary dependecies in package.json",
  "keywords": [
    "bin",
    "dependencies",
    "wget"
  ],
  "author": "n.m.batrakov",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/olive-branch/node-bin-manager"
  },
  "engines": {
    "node": ">= 11.0.0"
  },
  "main": "index.js",
  "bin": {
    "nbm": "./cli/index.js"
  },
  "scripts": {
    "start": "ts-node src/cli",
    "build": "tsc",
    "test": "jest",
    "lint": "eslint src --ext .js,.ts",
    "test:ci": "jest --testPathIgnorePatterns install.ispec",
    "test:unit": "jest -c jest.config.unit.js",
    "test:int": "jest -c jest.config.integration.js"
  },
  "dependencies": {
    "cliui": "^6.0.0",
    "lzma-native": "^6.0.0",
    "minimatch": "^3.0.0",
    "ora": "^4.0.0",
    "tar-stream": "^2.0.0",
    "yargs-parser": "^18.0.0",
    "yauzl": "^2.0.0"
  },
  "devDependencies": {
    "@types/jest": "^25.1.4",
    "@types/lzma-native": "^4.0.0",
    "@types/minimatch": "^3.0.3",
    "@types/node": "12.12.2",
    "@types/progress": "^2.0.3",
    "@types/tar-stream": "^2.1.0",
    "@types/yauzl": "^2.9.1",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "eslint": "^7.0.0",
    "eslint-config-airbnb-typescript": "^7.2.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prefer-let": "^1.0.1",
    "jest": "^25.2.3",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.8.3"
  }
}
